<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:KatalogFilm.ViewModel"
                    xmlns:view="clr-namespace:KatalogFilm.View">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MyFavoritesViewModel}">
        <view:MyFavorites/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AccountViewModel}">
        <view:Account/>
    </DataTemplate>
    
</ResourceDictionary>